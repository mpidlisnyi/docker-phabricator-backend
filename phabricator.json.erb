{
	"phabricator.base-uri": "http://phabricator.mycompany.com/",
<%- if ENV['MYSQL_HOST'] -%>
	"mysql.host": "<%= ENV['MYSQL_HOST'] %>",
<%- end -%>
<%- if ENV['MYSQL_USER'] -%>
	"mysql.user": "<%= ENV['MYSQL_USER'] %>",
<%- end -%>
<%- if ENV['MYSQL_PASS'] -%>
	"mysql.pass": "<%= ENV['MYSQL_PASS'] %>",
<%- end -%>
	"mysql.port": "<%= ENV['MYSQL_PORT'] ? ENV['MYSQL_PORT'] : 3306 %>"
}
